#include<stdio.h>
#include<stdlib.h>
int main()
{
    int a,b;
    printf("ENTER THE VALUES a and b are-");
    scanf("%d%d",&a,&b);
    printf("\nBEFORE SWAPPING\ta=%d,\tb=%d",a,b);
    //if a=4and b=5
    a=a*b;//4*5=9
    b=a/b;//9/4=5
    a=a/b;//9/5=4
    printf("\nAFTER SWAPPING\ta=%d,\tb=%d",a,b);
    return 0;
}
